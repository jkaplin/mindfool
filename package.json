{
  "name": "phaser-parcel",
  "version": "1.0.0",
  "homepage": "https://jkaplin.github.io/mindfool/",
  "description": "A Phaser 3 game template using Parcel bundler",
  "main": "src/index.html",
  "scripts": {
    "predeploy": "rm -rf dist && parcel build src/index.html",
    "deploy": "gh-pages -d dist",
    "build": "parcel build src/index.html --public-url ./",
    "watch": "parcel watch src/index.html",
    "start": "parcel serve src/index.html",
    "clean": "rm -v dist/*"
  },
  "keywords": [
    "phaser",
    "phaser3",
    "phaser-boilerplate"
  ],
  "author": "samme",
  "license": "ISC",
  "dependencies": {
    "phaser": "^3.20.1"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "parcel-bundler": "^1.12.4"
  }
}
